{
    "name": "secret-share",
    "version": "0.1.0",
    "private": true,
    "homepage": "",
    "license": "MIT",
    "scripts": {
        "build": "react-app-rewired build",
        "lint": "check-package-lock && shellcheck-all && tsc --noEmit && eslint . && prettier --loglevel warn --check .",
        "lint:fix": "eslint --fix . && prettier --loglevel warn --write .",
        "logs": "docker compose logs -f web",
        "start": "docker compose up -d --build --remove-orphans && npm run logs",
        "stop": "docker compose down -v",
        "test": "react-app-rewired test --watchAll=false",
        "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
        "test:watch": "react-app-rewired test --watchAll"
    },
    "dependencies": {
        "@fontsource/roboto-mono": "^4.5.8",

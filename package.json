        "@pentible/prettier": "0.53.0",
        "@pentible/shellcheck-all": "0.53.0",
        "@pentible/tsconfig": "0.53.0",
        "node-polyfill-webpack-plugin": "2.0.1",
        "react-app-alias-ex": "2.1.0",
        "react-app-rewired": "2.2.1"
    }

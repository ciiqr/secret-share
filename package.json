        "ghspa": "^1.0.0",
        "http": "npm:stream-http",
        "https": "npm:https-browserify",
        "path": "npm:path-browserify",
        "process": "0.11.10",
        "qrcode.react": "^1.0.1",
        "querystring": "npm:querystring-es3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hot-toast": "^2.4.0",
        "react-router-dom": "^6.6.2",
        "react-scripts": "5.0.1",
